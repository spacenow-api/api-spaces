{
  "name": "api-spaces",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "build": "tsc",
    "debug": "tsnd --inspect --respawn src/server.ts",
    "start": "tsn ./src/server.ts",
    "dev": "NODE_ENV=development tsnd ./src/server.ts",
    "prod": "NODE_ENV=production npx pm2-runtime start pm2.yaml",
    "migrate:up": "db-migrate up -c 999 --config ./src/helpers/config/database.json -m ./src/helpers/migrations",
    "migrate:down": "db-migrate down -c 999 --config ./src/helpers/config/database.json -m ./src/helpers/migrations",
    "db:prepare": "yarn migrate:down && yarn migrate:up"
  },
  "dependencies": {
    "await-to-js": "~2.1.1",
    "aws-sdk": "~2.488.0",
    "axios": "~0.19.0",
    "bcryptjs": "~2.4.3",
    "body-parser": "~1.19.0",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "crypto-js": "^3.1.9-1",
    "date-fns": "~1.30.1",
    "db-migrate": "~0.11.6",
    "dotenv": "~8.0.0",
    "express": "~4.17.0",
    "jsonwebtoken": "~8.5.1",
    "multer": "~1.4.1",
    "multer-s3": "~2.9.0",
    "multer-sharp-s3": "~0.1.0",
    "mysql2": "~1.6.5",
    "node-cache": "~5.1.0",
    "reflect-metadata": "~0.1.13",
    "sequelize": "^5.21.3",
    "sequelize-typescript": "^1.1.0",
    "sharp": "~0.20.8",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "~2.4.2",
    "@types/bluebird": "~3.5.27",
    "@types/compression": "~1.0.1",
    "@types/cookie-parser": "~1.4.1",
    "@types/crypto-js": "^3.1.43",
    "@types/dotenv": "~6.1.1",
    "@types/express": "~4.16.1",
    "@types/jsonwebtoken": "~8.3.2",
    "@types/multer": "~1.3.7",
    "@types/multer-s3": "~2.7.6",
    "@types/sequelize": "^4.28.8",
    "@types/sharp": "~0.17.10",
    "@types/uuid": "~3.4.4",
    "@types/validator": "~10.11.1",
    "db-migrate-mysql": "~1.1.10",
    "ts-node-dev": "~1.0.0-pre.39",
    "typescript": "~3.4.5"
  }
}
